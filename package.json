{
  "name": "desafio-processo-seletivo",
  "version": "0.0.1",
  "description": "Desafio do processo seletivo da INFO SISTEMAS.",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.1",
    "@types/chai": "^4.3.3",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/expect": "^24.3.0",
    "@types/express": "^4.17.13",
    "@types/mocha": "^10.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.45",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.2.0",
    "jest": "^27.3.1",
    "nodemon": "^2.0.19",
    "supertest": "^6.3.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "axios": "^0.22.0",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.5.1",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "http-status-codes": "^2.2.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.5.0",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.38",
    "yaml": "^2.1.1",
    "yamljs": "^0.3.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "dev": "NODE_ENV=dev nodemon -r tsconfig-paths/register --watch 'src/' --exec ts-node src/index.ts -e ts",
    "test": "NODE_ENV=test mocha -r ts-node/register \"test/**/*.spec.ts\" --exit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "typeorm": "ts-node ./node_modules/.bin/typeorm",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "typeorm:win": "ts-node-dev ./node_modules/typeorm/cli.js",
    "typeorm:create:win": "npm run typeorm:win migration:create -- -n",
    "typeorm:revert:win": "npm run typeorm:win migration:revert",
    "typeorm:run:win": "npm run typeorm:win migration:run"
  }
}
